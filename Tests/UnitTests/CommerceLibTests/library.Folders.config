<?xml version="1.0"?>
<!--
	Setup storage providers:
	   providers can be setup based on path, mimeType, size and extension, for example
		  <add path="\root\text\*">
			<add extension="txt" mimeType="text/plain" maxSize="1Kb" storageProvider="SqlBlobStorageProvider"/>
			<add mimeType="image/*" maxSize="200Kb" storageProvider="DiskBlobStorageProvider"/>
		  </add>
-->
<folderElementLocations>
  <add path="\*">
    <add storageProvider="PublicDiskBlobStorageProvider"/>
  </add>
</folderElementLocations>